$pageWidth = 1190px;

.box_title
  padding-bottom: 15px
  position: relative
  .r_box
    float: right

.box_title_label
  font-weight: bold
  font-size: 20px
  vertical-align: middle
  margin-left:10px
  margin-right:20px

.more_link
  color:#999
  margin-left:30px

.run_second
  position: absolute
  left:calc(50% - 116px)
  top:15px

.seckill_box
  width:$pageWidth
  margin-left: auto
  margin-right: auto
  .box_title
    padding-top:15px



.seckill_list
  display: flex
  align-items: center
  flex-wrap: wrap
  justify-content: center
  width: 1220px
  margin: 0 auto
  &:after
    content:''
    display: flex
    align-items: center
    clear:both
    height:0
    overflow:hidden